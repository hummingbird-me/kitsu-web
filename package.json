  "scripts": {
    "dev": "vite",
    "dev:storybook": "start-storybook -p 6006",
    "build": "npm run build:client && npm run build:server && npm run build:library && npm run build:storybook",
    "build:client": "BUILD_TARGET=client vite build",
    "build:server": "BUILD_TARGET=server vite build",
    "build:library": "BUILD_TARGET=library vite build",
    "build:storybook": "build-storybook --modern --output-dir ./dist/client/docs/ui/",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest watch",
    "test:e2e": "cypress",
    "codegen": "npm run codegen:intl && npm run codegen:graphql",
    "codegen:intl": "ts-node ./bin/extract.ts",
    "codegen:graphql": "graphql-codegen --config codegen.yml"
  },
